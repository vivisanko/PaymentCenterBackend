{
  "root": true,
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "linebreak-style": 0,
    "no-console": 0,
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_"
      }
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allow": ["_id"]
      }
    ]
  },
  "env": {
    "node": true
  }
}
